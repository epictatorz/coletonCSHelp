#include <iostream>
using namespace std;

class rational
{
  int hn, n1, n2, numer, deener;
  
 public:
  int add(n1, n2);
  int subtract(n1, n2);
  int multiply(n1, n2);
  int divide(n1, n2);
  bool equal(n1, n2);
  bool less(n1, n2);
  void getcomponents(n1, hn, numer, deener);
  void print(n1);
  
}
